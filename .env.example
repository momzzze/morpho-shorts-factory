# Example environment variables for Morpho Shorts Factory
# Copy to .env and fill in your secrets

# Core API
PORT=5001
RABBIT_URL=amqp://localhost:5672
CORS_ORIGINS=http://localhost:3000
DATABASE_URL=postgresql://user:pass@localhost:5432/morpho

# Storage
STORAGE_DRIVER=local
S3_BUCKET=
S3_ACCESS_KEY=
S3_SECRET_KEY=
S3_REGION=

# AI Service
AI_SERVICE_URL=http://localhost:8000

# TikTok/YouTube API
TIKTOK_API_KEY=
YOUTUBE_API_KEY=

# Add any new required envs below
