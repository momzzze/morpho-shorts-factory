                            "host": [
                                "{{baseUrl}}"
                            ],
                            "path": [
                                "api",
                                "v1",
                                "test",
                                "youtube",
                                "football highlights"
                            ],
                            "query": [
                                {
                                    "key": "count",
                                    "value": "10"
                                }
                            ]
                        }
                    }
                }
            ]
        }
    ]
}